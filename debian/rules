#!/usr/bin/make -f

export DH_VERBOSE=1

%:
	dh $@

override_dh_shlibdeps:
	dh_shlibdeps

override_dh_installman:
	find man -type f -name "*.adoc" -print0 -exec a2x -v -L -f manpage {} \;
	dh_installman

